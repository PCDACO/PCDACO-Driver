{
  "expo": {
    "name": "PCDACO-Driver",
    "slug": "PCDACO-Driver",
    "version": "1.0.0",
    "scheme": "PCDACO-Driver",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo-driver.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow PCDACO-Driver to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow PCDACO-Driver to access your camera",
          "microphonePermission": "Allow PCDACO-Driver to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/logo-driver.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo-driver.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anhden123.PCDACODriver",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-driver.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anhden123.PCDACODriver"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d2eb9b3e-568e-48ab-8c32-b2f8cc2466b5"
      }
    }
  }
}